<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="通过userID查询用户信息" parallel="classes" thread-count="4">

    <parameter name="serviceEnv" value="https://user-center-thu.tunaikita.id/user"/>

    <test name="userQueryUserInfoByUserId" preserve-order="true">
        <classes>
            <class name="com.wecash.http.cases.userQueryUserInfoByUserId">
                <methods>
                    <include name="GetInfoByUserId" />
                </methods>
            </class>
        </classes>
    </test>

    <listeners>
        <listener class-name="com.wecash.http.utils.ExtentTestNGIReporterListener" />
        <!--<listener class-name="com.wecash.http.utils.ExtentTestNgFormatter" />-->
    </listeners>
</suite>